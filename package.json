{
  "name": "vueless-quickstart-ts",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 4310",
    "build": "vite build",
    "preview": "vite preview --host --port 4310",
    "lint": "eslint --no-fix src/ .storybook/ .vueless/",
    "lint:fix": "eslint --fix src/ .storybook/ .vueless/",
    "lint:ci": "eslint --no-fix --max-warnings=0",
    "ts:check": "vue-tsc --build --force",
    "test": "vitest",
    "test:ci": "vitest --run",
    "sb:dev": "storybook dev -p 6006 --no-open",
    "sb:dev:docs": "storybook dev -p 6006 --docs --no-open",
    "sb:build": "storybook build --docs",
    "sb:preview": "vite preview --host --outDir=storybook-static"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.13.2",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.4",
    "qs": "^6.14.0",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.4",
    "vueless": "^1.4.1"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^2.0.6",
    "@rollup/plugin-yaml": "^4.1.2",
    "@tsconfig/node20": "^20.1.8",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.2",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/eslint-plugin": "^1.5.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vueless/storybook": "^1.4.11",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "^10.1.9",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.8"
  },
  "engines": {
    "node": ">=20"
  }
}
