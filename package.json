{
  "name": "vueless-quickstart-ts",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 4310",
    "build": "vite build",
    "preview": "vite preview --host --port 4310",
    "lint": "eslint --no-fix src/ .storybook/ .vueless/",
    "lint:fix": "eslint --fix src/ .storybook/ .vueless/",
    "lint:ci": "eslint --no-fix --max-warnings=0",
    "ts:check": "vue-tsc --build --force",
    "test": "vitest",
    "test:ci": "vitest --run",
    "sb:dev": "storybook dev -p 6006 --no-open",
    "sb:dev:docs": "storybook dev -p 6006 --docs --no-open",
    "sb:build": "storybook build --docs",
    "sb:preview": "vite preview --host --outDir=storybook-static"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.10.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.10",
    "vue-router": "^4.5.1",
    "vueless": "^1.3.4"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@nabla/vite-plugin-eslint": "^2.0.6",
    "@tsconfig/node20": "^20.1.6",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.1.0",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vueless/storybook": "^1.3.4",
    "eslint": "^9.31.0",
    "eslint-plugin-storybook": "^9.0.17",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  },
  "engines": {
    "node": ">=20"
  }
}
